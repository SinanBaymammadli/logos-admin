{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Posts",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "cover_image": {
      "model": "file",
      "via": "related",
      "allowedTypes": ["images"],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true
    }
  }
}
