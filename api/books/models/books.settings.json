{
  "kind": "collectionType",
  "collectionName": "books",
  "info": {
    "name": "Books",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true,
      "default": 0,
      "min": 0
    },
    "description": {
      "type": "text"
    },
    "cover_image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "detail_images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "isbn": {
      "type": "string"
    },
    "level": {
      "type": "enumeration",
      "enum": [
        "Beginner",
        "Elementary",
        "Preintermediate",
        "Intermediate",
        "Upperintermediate",
        "Advanced",
        "Derece1",
        "Derece2",
        "Derece3",
        "A1",
        "A2",
        "B1",
        "B2",
        "C1",
        "C2"
      ]
    },
    "from_lang": {
      "type": "enumeration",
      "enum": [
        "Azerbaijani",
        "English",
        "Russian",
        "German"
      ],
      "required": true
    },
    "to_lang": {
      "type": "enumeration",
      "enum": [
        "Azerbaijani",
        "English",
        "Russian",
        "German"
      ],
      "required": true
    },
    "categories": {
      "collection": "categories",
      "via": "books",
      "dominant": true
    },
    "author": {
      "model": "authors",
      "via": "books"
    }
  }
}
